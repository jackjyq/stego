{
  "name": "Stego Nightmare",  
  "colors": {
    // Base Colors
    // https://code.visualstudio.com/api/references/theme-color#base-colors
    "focusBorder": "__TRANSPARENT",
    // Editor colors
    "editor.background": "__ANSI_BLACK",
    "editor.foreground": "__HIGH_EMPHASIS",
    "editorCursor.foreground": "__WHITE",
    "editorRuler.foreground": "__MEDIUM_EMPHASIS",
    "editorLineNumber.activeForeground": "__HIGH_EMPHASIS",
    "editorLineNumber.foreground": "__MEDIUM_EMPHASIS",
    // Peek view colors
    // https://code.visualstudio.com/api/references/theme-color#peek-view-colors
    "peekView.border": "__UI_24_DP",
    "peekViewEditor.background": "__UI_08_DP",
    "peekViewResult.background": "__UI_12_DP",
    "peekViewResult.fileForeground": "__HIGH_EMPHASIS",
    "peekViewResult.lineForeground": "__HIGH_EMPHASIS",
    "peekViewTitle.background": "__UI_24_DP",
    "peekViewTitleLabel.foreground": "__HIGH_EMPHASIS",
    "peekViewTitleDescription.foreground": "__MEDIUM_EMPHASIS",
    // Title Bar colors
    // https://code.visualstudio.com/api/references/theme-color#title-bar-colors
    "titleBar.activeBackground": "__UI_24_DP",
    "titleBar.activeForeground": "__HIGH_EMPHASIS",
    "titleBar.inactiveBackground": "__UI_24_DP",
    "titleBar.inactiveForeground": "__MEDIUM_EMPHASIS",
    // Status Bar colors
    // https://code.visualstudio.com/api/references/theme-color#status-bar-colors
    "statusBar.background": "__UI_24_DP",
    "statusBar.foreground": "__HIGH_EMPHASIS",
    "statusBar.focusBorder": "__TRANSPARENT",
    "statusBar.border": "__TRANSPARENT",
    "statusBar.debuggingBackground": "__UI_24_DP",
    "statusBar.debuggingForeground": "__HIGH_EMPHASIS",
    "statusBar.debuggingBorder": "__TRANSPARENT",
    "statusBar.noFolderBackground": "__UI_24_DP",
    "statusBar.noFolderForeground": "__HIGH_EMPHASIS",
    "statusBar.noFolderBorder": "__TRANSPARENT",
    // Debug Icons colors
    // https://code.visualstudio.com/api/references/theme-color#editor-groups-tabs
    "debugConsole.errorForeground": "__ANSI_RED",
    "debugConsole.warningForeground": "__ANSI_YELLOW",
    "debugConsole.infoForeground": "__ANSI_GREEN",
    "debugConsole.sourceForeground": "__HIGH_EMPHASIS",
    // Editor Groups & Tabs
    // https://code.visualstudio.com/api/references/theme-color#editor-groups-tabs
    "editorGroupHeader.tabsBackground": "__UI_04_DP",
    "tab.activeBackground": "__UI_12_DP",
    "tab.activeForeground": "__HIGH_EMPHASIS",
    "tab.inactiveBackground": "__UI_04_DP",
    "tab.inactiveForeground": "__HIGH_EMPHASIS",
    "tab.activeModifiedBorder": "__ANSI_YELLOW",
    "tab.hoverBackground": "__UI_12_DP",
    // Breadcrumbs colors
    // https://code.visualstudio.com/api/references/theme-color#breadcrumbs-colors
    "breadcrumb.background": "__UI_12_DP",
    "breadcrumb.foreground": "__MEDIUM_EMPHASIS",
    // Side Bar colors
    // https://code.visualstudio.com/api/references/theme-color#side-bar
    "sideBar.background": "__UI_03_DP",
    "sideBar.foreground": "__HIGH_EMPHASIS",
    "sideBarSectionHeader.background": "__UI_12_DP",
    "sideBarSectionHeader.foreground": "__HIGH_EMPHASIS",
    // Activity Bar colors
    // https://code.visualstudio.com/api/references/theme-color#activity-bar
    "activityBar.background": "__UI_24_DP",
    "activityBar.foreground": "__HIGH_EMPHASIS",
    "activityBar.inactiveForeground": "__MEDIUM_EMPHASIS",
    "activityBar.activeBorder": "__HIGH_EMPHASIS",
    // Panel Colors
    // https://code.visualstudio.com/api/references/theme-color#panel-colors
    "panel.background": "__UI_03_DP",
    "panelTitle.activeForeground": "__HIGH_EMPHASIS",
    "panelTitle.inactiveForeground": "__MEDIUM_EMPHASIS",
    "panelTitle.activeBorder": "__HIGH_EMPHASIS",
    // Terminal Colors
    "terminal.foreground": "__HIGH_EMPHASIS",
    "terminal.background": "__ANSI_BLACK",
    "terminal.ansiBlack": "__ANSI_BLACK",
    "terminal.ansiBlue": "__ANSI_BLUE",
    "terminal.ansiCyan": "__ANSI_CYAN",
    "terminal.ansiGreen": "__ANSI_GREEN",
    "terminal.ansiMagenta": "__ANSI_MAGENTA",
    "terminal.ansiRed": "__ANSI_RED",
    "terminal.ansiWhite": "__HIGH_EMPHASIS",
    "terminal.ansiYellow": "__ANSI_YELLOW",
    "terminal.ansiBrightBlack": "__ANSI_BLACK",
    "terminal.ansiBrightBlue": "__ANSI_BLUE",
    "terminal.ansiBrightCyan": "__ANSI_CYAN",
    "terminal.ansiBrightGreen": "__ANSI_GREEN",
    "terminal.ansiBrightMagenta": "__ANSI_MAGENTA",
    "terminal.ansiBrightRed": "__ANSI_RED",
    "terminal.ansiBrightWhite": "__HIGH_EMPHASIS",
    "terminal.ansiBrightYellow": "__ANSI_YELLOW",
    "terminal.selectionBackground": "__SELECTION_BACKGROUND",
    "terminalCursor.foreground": "__WHITE"
  },
  "tokenColors": [
    // Base Colors
    {
      "name": "Comment",
      "scope": ["comment", "string.quoted.docstring"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "__MEDIUM_EMPHASIS"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "__ANSI_MAGENTA"
      }
    },
    {
      "name": "Entity",
      "scope": ["entity"],
      "settings": {
        "fontStyle": "",
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": " bold",
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "__ANSI_YELLOW"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "__ANSI_CYAN"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Entity inherited-class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "String embedded-source",
      "scope": "string.quoted source",
      "settings": {
        "fontStyle": "",
        "foreground": "__ANSI_YELLOW"
      }
    },
    {
      "name": "String constant",
      "scope": "string constant",
      "settings": {
        "foreground": "__ANSI_MAGENTA"
      }
    },
    {
      "name": "String.regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "String variable",
      "scope": "string variable",
      "settings": {
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "Support.function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "Support.constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "C/C++ Preprocessor Line",
      "scope": "other.preprocessor.c",
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "C/C++ Preprocessor Directive",
      "scope": "other.preprocessor.c entity",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "Javascript object literal separator",
      "scope": "punctuation.separator.key-value.js",
      "settings": {
        "foreground": "__ANSI_MAGENTA"
      }
    },
    {
      "name": "Javascript object literal key",
      "scope": "meta.object-literal.key.js",
      "settings": {
        "foreground": "__ANSI_YELLOW"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "Meta.tag.A",
      "scope": [
        "declaration.tag",
        "declaration.tag entity",
        "meta.tag",
        "meta.tag entity"
      ],
      "settings": {
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "css tag-name",
      "scope": "meta.selector.css entity.name.tag",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "css#id",
      "scope": "meta.selector.css entity.other.attribute-name.id",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "css.class",
      "scope": "meta.selector.css entity.other.attribute-name.class",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "css property-name:",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "__ANSI_BLUE",
        "fontStyle": ""
      }
    },
    {
      "name": "css property-value;",
      "scope": [
        "meta.property-group support.constant.property-value.css",
        "meta.property-value support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "__ANSI_GREEN",
        "fontStyle": " italic"
      }
    },
    {
      "name": "css @at-rule",
      "scope": "meta.preprocessor.at-rule keyword.control.at-rule",
      "settings": {
        "foreground": "__ANSI_YELLOW",
        "fontStyle": " bold"
      }
    },
    {
      "name": "css additional-constants",
      "scope": [
        "meta.property-value support.constant.named-color.css",
        "meta.property-value constant"
      ],
      "settings": {
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "css constructor.argument",
      "scope": "meta.constructor.argument.css",
      "settings": {
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "__ANSI_YELLOW"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "markdown heading",
      "scope": ["punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "markdown puncuation",
      "scope": ["punctuation.definition.markdown"],
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "html id",
      "scope": "entity.other.attribute-name.id.html",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "html class",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "html punctuations tags",
      "scope": [
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "CSS @import",
      "scope": "keyword.control.at-rule.import.css",
      "settings": {
        "foreground": "__ANSI_YELLOW",
        "fontStyle": " bold"
      }
    },
    {
      "name": "LESS variables",
      "scope": "variable.other.less",
      "settings": {
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "LESS variables",
      "scope": "entity.other.less.mixin",
      "settings": {
        "foreground": "__ANSI_MAGENTA"
      }
    },
    {
      "name": "LESS units",
      "scope": "source.css.less keyword.unit.css",
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "Angular",
      "scope": [
        "entity.other.attribute-name.angular.html",
        "source.angular.embedded.html"
      ],
      "settings": {
        "foreground": "__ANSI_RED"
      }
    },
    {
      "name": "HTML Entities",
      "scope": "constant.character.entity.html",
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "coffee instance var",
      "scope": "variable.other.readwrite.instance.coffee",
      "settings": {
        "foreground": "__ANSI_YELLOW"
      }
    },
    {
      "name": "coffee brace",
      "scope": ["meta.brace.round.coffee", "meta.brace.square.coffee"],
      "settings": {
        "foreground": "__ANSI_YELLOW"
      }
    },
    {
      "name": "coffee embeded",
      "scope": "punctuation.section.embedded.coffee",
      "settings": {
        "foreground": "__ANSI_CYAN"
      }
    },
    {
      "name": "Coffee vars",
      "scope": "variable.assignment.coffee variable.assignment.coffee",
      "settings": {
        "foreground": "__HIGH_EMPHASIS"
      }
    },
    {
      "name": "Coffee dots",
      "scope": "meta.delimiter.method.period.coffee",
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    },
    {
      "name": "Coffee curleys",
      "scope": "meta.brace.curly.coffee",
      "settings": {
        "foreground": "__ANSI_GREEN"
      }
    },
    {
      "name": "Doctype/XML Processing",
      "scope": [
        "meta.tag.sgml.doctype.xml",
        "declaration.sgml.html declaration.doctype",
        "declaration.sgml.html declaration.doctype entity",
        "declaration.sgml.html declaration.doctype string",
        "declaration.xml-processing",
        "declaration.xml-processing entity",
        "declaration.xml-processing string",
        "doctype"
      ],
      "settings": {
        "foreground": "__ANSI_BLUE"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // https://code.visualstudio.com/api/language-extensions/semantic-highlight-guide#semantic-token-scope-map
    "namespace": {
      // For identifiers that declare or reference a namespace, module, or package.
      "foreground": "__ANSI_GREEN"
    },
    "type": {
      "foreground": "__ANSI_GREEN"
    },
    "type.defaultLibrary": {
      "foreground": "__ANSI_CYAN"
    },
    "struct": {
      "foreground": "__ANSI_BLUE"
    },
    "class": {
      "foreground": "__ANSI_GREEN"
    },
    "interface": {
      "foreground": "__ANSI_GREEN"
    },
    "enum": {
      "foreground": "__ANSI_GREEN"
    },
    "function": {
      "foreground": "__ANSI_GREEN"
    },
    "function.defaultLibrary": {
      "foreground": "__ANSI_CYAN"
    },
    "method": {
      "foreground": "__ANSI_GREEN"
    },
    "macro": {
      "foreground": "__ANSI_GREEN"
    },
    "variable": {
      "foreground": "__HIGH_EMPHASIS"
    },
    "variable.readonly": {
      "foreground": "__HIGH_EMPHASIS"
    },
    "variable.readonly:python": {
      "foreground": "__ANSI_MAGENTA"
    },
    "parameter": {
      "foreground": "__HIGH_EMPHASIS"
    },
    "property": {
      "foreground": "__HIGH_EMPHASIS"
    },
    "property.readonly": {
      "foreground": "__HIGH_EMPHASIS"
    },
    "enumMember": {
      "foreground": "__HIGH_EMPHASIS"
    },
    "event": {
      "foreground": "__HIGH_EMPHASIS"
    },
    //TypeHint Patch, keep it to the bottom
    "*.typeHint": "__MEDIUM_EMPHASIS"
  }
}